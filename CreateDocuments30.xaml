<Page
    x:Class="Мастерконд.Документы.CreateDocuments30"
    xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
    xmlns:local="using:Мастерконд.Документы"
    xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
    xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
    mc:Ignorable="d" Background="{ThemeResource ApplicationPageBackgroundThemeBrush}">
    

    <ScrollViewer ScrollViewer.HorizontalScrollMode="Auto" ScrollViewer.VerticalScrollMode="Auto" ScrollViewer.HorizontalScrollBarVisibility="Auto" ScrollViewer.VerticalScrollBarVisibility="Auto" Background="White">
        <RelativePanel x:Name="MainRelativePanel" ScrollViewer.HorizontalScrollMode="Enabled" ScrollViewer.VerticalScrollMode="Auto" ScrollViewer.HorizontalScrollBarVisibility="Auto">
            <TextBlock x:Name="TextBlockCreateDocuments5" Text="СОЗДАТЬ КОМПЛЕКТ ДОКУМЕНТОВ ДЛЯ СДЕЛКИ"  FontSize="16" FontWeight="SemiBold" Margin="20,10,0,0" Width="700" Foreground="Green"/>
            <TextBlock x:Name="TextBlockCreateDocumentsOpisanie5" Text="В этом разделе можно создать комплект документов на поставку товаров и оказание услуг по модели СМЕТА (коммерческое предложение) -> СЧЕТ -> ДОГОВОР -> ТОВАРНАЯ НАКЛАДНАЯ -> АКТ. Эта модель подходит для большинства небольших организаций, работающих в сфере строительных, монтажных и сервисно-ремонтных услуг и находящихся на Упрощенной Системе Налогообложения (УСН). Данный раздел служит для быстрого создания пакета документов для не более чем 30 наименований товаров или услуг. Не используйте в полях ввода данных в этом разделе знак &quot;;&quot; - он является разделителем в сохраняемом файле сделки (.docdata)." TextWrapping="WrapWholeWords"  Margin="20,15,0,0" RelativePanel.Below="TextBlockCreateDocuments5" Width="700"/>

            <Button x:Name="LoadNastrojki5" Content="Загрузить файл сделки (.docdata)" RelativePanel.Below="TextBlockCreateDocumentsOpisanie5" Margin="20,20,0,0" Click="LoadNastrojki5_Click" Background="#99BC64" Foreground="White"/>
            <TextBlock x:Name="TextBlockLoadNastrojkiMessage" RelativePanel.Below="LoadNastrojki5" Margin="20,20,0,0"  FontWeight="SemiBold" Foreground="Red" Width="300"  TextWrapping="WrapWholeWords"/>

            <TextBlock x:Name="TextBlock51" FontWeight="SemiBold" Text="РЕКВИЗИТЫ КОНТРАГЕНТА" RelativePanel.Below="TextBlockLoadNastrojkiMessage" Margin="20,15,0,0"  FontSize="15"/>
            <Button x:Name="LoadRekv5" Content="Загрузить реквизиты контрагента" RelativePanel.Below="TextBlock51" Margin="20,20,0,0" Click="LoadRekviziti5_Click"/>
            <TextBlock x:Name="TextBlockFullNameKontr5" Text="Наименование контрагента (полностью):"  RelativePanel.Below="LoadRekv5" Margin="20,15,0,0" />
            <TextBox x:Name="TextBoxFullNameKontr5"  RelativePanel.Below="TextBlockFullNameKontr5" Margin="20,5,0,0" Width="300" />
            <TextBlock x:Name="TextBlockSokrNameKontr5" Text="Наименование контрагента (сокращенно):"  RelativePanel.Below="TextBoxFullNameKontr5" Margin="20,15,0,0" />
            <TextBox x:Name="TextBoxSokrNameKontr5"  RelativePanel.Below="TextBlockSokrNameKontr5" Margin="20,5,0,0" Width="300" />
            <TextBlock x:Name="TextBlockINN5" RelativePanel.Below="TextBoxSokrNameKontr5" Text="ИНН контрагента:"  Margin="20,15,0,0" />
            <TextBox x:Name="TextBoxINN5" RelativePanel.Below="TextBlockINN5" Margin="20,5,0,0" Width="300" />
            <TextBlock x:Name="TextBlockKPP5" RelativePanel.Below="TextBoxINN5" Margin="20,15,0,0" Text="КПП контрагента:"  />
            <TextBox x:Name="TextBoxKPP5" Width="300"  RelativePanel.Below="TextBlockKPP5" Margin="20,5,0,0" />
            <TextBlock x:Name="TextBlockOGRN5" Text="ОГРН контрагента:"  RelativePanel.Below="TextBoxKPP5" Margin="20,15,0,0"/>
            <TextBox x:Name="TextBoxOGRN5" RelativePanel.Below="TextBlockOGRN5" Margin="20,5,0,0" Width="300" />
            <TextBlock x:Name="TextBlockBankRekv5" Text="Банковские реквизиты:"  Margin="20,15,0,0" RelativePanel.Below="TextBoxOGRN5" />
            <TextBox x:Name="TextBoxBankRekv5" RelativePanel.Below="TextBlockBankRekv5" Width="300"  Margin="20,5,0,0" />
            <TextBlock x:Name="TextBlockDolzhnostDirector5" RelativePanel.Below="TextBoxBankRekv5" Text="Должность руководителя:" Margin="20,15,0,0"  />
            <TextBox x:Name="TextBoxDolzhnostDirector5" RelativePanel.Below="TextBlockDolzhnostDirector5" Margin="20,5,0,0" Width="300" />
            <TextBlock x:Name="TextBlockVlice5" Width="300"  Height="60" TextWrapping="WrapWholeWords" Text="В лице ...(например, Генерального директора Петрова Ивана Ивановича, действующего на основании Устава):" Margin="20,15,0,0" RelativePanel.Below="TextBoxDolzhnostDirector5"/>
            <TextBox x:Name="TextBoxVlice5" Width="300"  RelativePanel.Below="TextBlockVlice5" Margin="20,5,0,0"/>
            <TextBlock x:Name="TextBlockRukSokr5" Text="ФИО руководителя(сокращенно):"  Margin="20,15,0,0" RelativePanel.Below="TextBoxVlice5" />
            <TextBox x:Name="TextBoxRukSokr5" Width="300"  RelativePanel.Below="TextBlockRukSokr5" Margin="20,5,0,0"/>
            <TextBlock x:Name="TextBlockUrAddr5"  Margin="20,15,0,0" Text="Юридический адрес:" RelativePanel.Below="TextBoxRukSokr5" />
            <TextBox x:Name="TextBoxUrAddr5" Margin="20,5,0,0" RelativePanel.Below="TextBlockUrAddr5" Width="300" />
            <TextBlock x:Name="TextBlockFactAddr5" Margin="20,15,0,0"  RelativePanel.Below="TextBoxUrAddr5" Text="Фактический адрес:" />
            <TextBox x:Name="TextBoxFactAddr5" Margin="20,5,0,0" RelativePanel.Below="TextBlockFactAddr5" Width="300"  />
            <TextBlock x:Name="TextBlockPhone5" Margin="20,15,0,0"  RelativePanel.Below="TextBoxFactAddr5" Text="Телефон:"/>
            <TextBox x:Name="TextBoxPhone5" Width="300"  Margin="20,5,0,0" RelativePanel.Below="TextBlockPhone5"/>
            <TextBlock x:Name="TextBlockFIOOtvetstvennij5" Margin="20,15,0,0"  RelativePanel.Below="TextBoxPhone5" Text="ФИО ответственного со стороны Заказчика за исполнение Договора:" TextWrapping="WrapWholeWords" Width="300" />
            <TextBox x:Name="TextBoxFIOOtvetstvennij5" Width="300"  Margin="20,5,0,0" RelativePanel.Below="TextBlockFIOOtvetstvennij5"/>
            <TextBlock x:Name="TextBlockMobile5"  Text="Мобильный телефон ответственного:" RelativePanel.Below="TextBoxFIOOtvetstvennij5" Margin="20,15,0,0"/>
            <TextBox x:Name="TextBoxMobile5" Width="300"  RelativePanel.Below="TextBlockMobile5" Margin="20,5,0,0" />
            <TextBlock x:Name="TextBlockEmail5" Text="Электронная почта:"  RelativePanel.Below="TextBoxMobile5" Margin="20,15,0,0"/>
            <TextBox x:Name="TextBoxEmail5" Width="300"  Margin="20,5,0,0" RelativePanel.Below="TextBlockEmail5" />
            <TextBlock x:Name="TextBlockSajt5"  RelativePanel.Below="TextBoxEmail5" Margin="20,15,0,0" Text="Сайт:" />
            <TextBox x:Name="TextBoxSajt5" Width="300"  RelativePanel.Below="TextBlockSajt5" Margin="20,5,0,0" />

            <Button x:Name="SaveNewRekviziti5" Content="Сохранить/обновить реквизиты контрагента" RelativePanel.Below="TextBoxSajt5" Click="SaveNewRekviziti5_Click" Margin="20,20,0,0"/>
            <TextBlock x:Name="TextBlockDanniePoDokumentam5" FontWeight="SemiBold" Text="ДАННЫЕ ДЛЯ ДОКУМЕНТОВ"  Margin="20,25,0,0" RelativePanel.Below="SaveNewRekviziti5" />
            <TextBlock x:Name="TextBlockDataSmeti5" Text="Дата составления сметы (коммерческого предложения):" TextWrapping="WrapWholeWords" Width="300"   Margin="20,15,0,0" RelativePanel.Below="TextBlockDanniePoDokumentam5" />
            <TextBox x:Name="TextBoxDataSmeti5" Width="300"  RelativePanel.Below="TextBlockDataSmeti5" Margin="20,5,0,0" />
            <TextBlock x:Name="TextBlockDataDejstvijaSmeti5" Text="Смета действительна до (например, 5 февраля 2016 года):" TextWrapping="WrapWholeWords" Width="300"   Margin="20,15,0,0" RelativePanel.Below="TextBoxDataSmeti5" />
            <TextBox x:Name="TextBoxDataDejstvijaSmeti5" Width="300"  RelativePanel.Below="TextBlockDataDejstvijaSmeti5" Margin="20,5,0,0" />
            <TextBlock x:Name="TextBlockDataDogovora5" Text="Дата договора и счета:"  Margin="20,15,0,0" RelativePanel.Below="TextBoxDataDejstvijaSmeti5"/>
            <TextBox x:Name="TextBoxDataDogovora5" Width="300"  RelativePanel.Below="TextBlockDataDogovora5" Margin="20,5,0,0"/>
            <TextBlock x:Name="TextBlockNomerDogovora5" Text="Номер договора/счета/акта/накладной:"  Margin="20,15,0,0" RelativePanel.Below="TextBoxDataDogovora5" />
            <TextBox x:Name="TextBoxNomerDogovora5" RelativePanel.Below="TextBlockNomerDogovora5" Width="300"  Margin="20,5,0,0"/>

            <TextBlock x:Name="TextBlockDataNakladnoj5" Text="Дата накладной:"  RelativePanel.Below="TextBoxNomerDogovora5" Margin="20,15,0,0" />
            <TextBox x:Name="TextBoxDataNakladnoj5" Width="300"  RelativePanel.Below="TextBlockDataNakladnoj5" Margin="20,5,0,0" />
            <TextBlock x:Name="TextBlockDataAkta5" Text="Дата акта:"  RelativePanel.Below="TextBoxDataNakladnoj5" Margin="20,15,0,0" />

            <TextBox x:Name="TextBoxDataAkta5" Width="300"  RelativePanel.Below="TextBlockDataAkta5" Margin="20,5,0,0" />

            <TextBlock x:Name="TextBlockSrokiIspolnenija5"  Text="Сроки поставки товаров, оказания услуг, п.2.2 Договора (например, в течение 5(пяти) дней с момента поступления заказа со стороны Заказчика):" TextWrapping="WrapWholeWords" Width="300"  RelativePanel.Below="TextBoxDataAkta5" Margin="20,15,0,0" />
            <TextBox x:Name="TextBoxSrokiIspolnenija5" Width="300"  RelativePanel.Below="TextBlockSrokiIspolnenija5" Margin="20,5,0,0" />
            <TextBlock x:Name="TextBlockUslovijaOplati5" Text="Условия по оплате товаров и услуг п.2.3 Договора (например, в течение 3 (трёх) рабочих дней после подписания Сторонами Акта):" RelativePanel.Below="TextBoxSrokiIspolnenija5" Margin="20,15,0,0"  Width="300"  TextWrapping="WrapWholeWords"/>
            <TextBox x:Name="TextBoxUslovijaOplati5" Width="300"  RelativePanel.Below="TextBlockUslovijaOplati5" Margin="20,5,0,0" />
            <TextBlock x:Name="TextBlockGarantija5" Text="Гарантийные обязательства (раздел 4 Договора):" TextWrapping="WrapWholeWords" Width="300"   RelativePanel.Below="TextBoxUslovijaOplati5" Margin="20,15,0,0" />
            <TextBox x:Name="TextBoxGarantija5" Width="300"  RelativePanel.Below="TextBlockGarantija5" Margin="20,5,0,0" />
            <TextBlock x:Name="TextBlockGorodZaklDogovora5" Text="Место заключения договора (например, г.Москва):" TextWrapping="WrapWholeWords" Width="300"   RelativePanel.Below="TextBoxGarantija5" Margin="20,15,0,0" />
            <TextBox x:Name="TextBoxGorodZaklDogovora5" Width="300"  RelativePanel.Below="TextBlockGorodZaklDogovora5" Margin="20,5,0,0" />
            <TextBlock x:Name="TextBlockOsobieUslovijaDogovora5" Text="Особые условия договора (раздел 7):"  RelativePanel.Below="TextBoxGorodZaklDogovora5" Margin="20,15,0,0" />
            <TextBox x:Name="TextBoxOsobieUslovijaDogovora5" Width="300"  Margin="20,5,0,0" RelativePanel.Below="TextBlockOsobieUslovijaDogovora5" />
            <TextBlock x:Name="TextBlockSud5"  Text="Название Арбитражного суда для рассмотрения споров (будет включено в п.8 Договора):" TextWrapping="WrapWholeWords" Width="300"  RelativePanel.Below="TextBoxOsobieUslovijaDogovora5" Margin="20,15,0,0" />
            <TextBox x:Name="TextBoxSud5" Width="300"  RelativePanel.Below="TextBlockSud5" Margin="20,5,0,0" />

            <TextBlock x:Name="TextBlockZero200" RelativePanel.Below="TextBoxSud5" Height="100" />

            <TextBlock x:Name="TextBlockTovari5" Text="ТОВАРЫ/УСЛУГИ" FontWeight="SemiBold"  FontSize="16" Margin="20,20,0,0" RelativePanel.Below="TextBlockZero200" />

            <TextBlock x:Name="TextBlockTovariNaimenivanie5" Text="Наименование товаров или услуг"  RelativePanel.Below="TextBlockTovari5" Margin="20,5,0,0"/>
            <TextBlock x:Name="TextBlockTovariKolichestvo5" Text="Кол-во" RelativePanel.RightOf="TextBlockTovariNaimenivanie5" RelativePanel.Below="TextBlockTovari5" Margin="20,5,0,0" />
            <TextBlock x:Name="TextBlockTovariEd5" Text="Ед."  RelativePanel.RightOf="TextBlockTovariKolichestvo5" RelativePanel.Below="TextBlockTovari5" Margin="55,5,0,0" />
            <TextBlock x:Name="TextBlockTovariCenaed5" Text="Цена за ед."  RelativePanel.RightOf="TextBlockTovariEd5" RelativePanel.Below="TextBlockTovari5" Margin="45,5,0,0" />
            <TextBlock x:Name="TextBlockTovariStoimost5" Text="Стоимость"  RelativePanel.RightOf="TextBlockTovariCenaed5" RelativePanel.Below="TextBlockTovari5" Margin="70,5,0,0" />
            <TextBlock x:Name="TextBlockTovariVNakladnaja5" Text="В накладную"  RelativePanel.RightOf="TextBlockTovariStoimost5" RelativePanel.Below="TextBlockTovari5" Margin="20,5,0,0" />

            <TextBox x:Name="TextBoxTovar1Naimenovanie5" RelativePanel.Below="TextBlockTovariNaimenivanie5" Width="230" Margin="20,15,0,0" />
            <TextBox x:Name="TextBoxTovar1Kolvo5" RelativePanel.Below="TextBlockTovariNaimenivanie5" RelativePanel.RightOf="TextBoxTovar1Naimenovanie5" Margin="20,15,0,0" />
            <TextBox x:Name="TextBoxTovar1Ed5" RelativePanel.Below="TextBlockTovariNaimenivanie5" RelativePanel.RightOf="TextBoxTovar1Kolvo5" Margin="20,15,0,0" />
            <TextBox x:Name="TextBoxTovar1CenaEd5" RelativePanel.Below="TextBlockTovariNaimenivanie5" RelativePanel.RightOf="TextBoxTovar1Ed5" Margin="20,15,0,0" Width="80" />
            <Button x:Name="ButtonTovar1RaschetStoimost5" Content=">" RelativePanel.Below="TextBlockTovariNaimenivanie5" RelativePanel.RightOf="TextBoxTovar1CenaEd5" Margin="20,15,0,0" Click="ButtonTovar1RaschetStoimost5_Click" />
            <TextBox x:Name="TextBoxTovar1Stoimost5" RelativePanel.Below="TextBlockTovariNaimenivanie5" RelativePanel.RightOf="ButtonTovar1RaschetStoimost5" Margin="20,15,0,20" Width="80" />
            <CheckBox x:Name="CheckBoxTovar1" RelativePanel.Below="TextBlockTovariNaimenivanie5" RelativePanel.RightOf="TextBoxTovar1Stoimost5" Margin="40,15,0,0" />
            <Button x:Name="AddTovar1Next" RelativePanel.Below="TextBoxTovar1Naimenovanie5" Margin="20,5,0,0" Background="#FF99BC64" Foreground="White" Content="Добавить товар/услугу" Click="AddTovar1Next_Click"/>
            <!--<Button x:Name="RemoveTovar1Next" RelativePanel.Below="TextBoxTovar1Naimenovanie5" RelativePanel.RightOf="AddTovar1Next" Margin="20,5,0,0" Background="#FF99BC64" Foreground="White" Content="Убрать товар/услугу"/>-->

            <TextBox x:Name="TextBoxTovar2Naimenovanie5" RelativePanel.Below="TextBoxTovar1Naimenovanie5" Width="230" Margin="20,15,0,0" />
            <TextBox x:Name="TextBoxTovar2Kolvo5" RelativePanel.Below="TextBoxTovar1Naimenovanie5" RelativePanel.RightOf="TextBoxTovar1Naimenovanie5" Margin="20,15,0,0" />
            <TextBox x:Name="TextBoxTovar2Ed5" RelativePanel.Below="TextBoxTovar1Naimenovanie5" RelativePanel.RightOf="TextBoxTovar1Kolvo5" Margin="20,15,0,0" />
            <TextBox x:Name="TextBoxTovar2CenaEd5" RelativePanel.Below="TextBoxTovar1Naimenovanie5" RelativePanel.RightOf="TextBoxTovar1Ed5" Margin="20,15,0,0" Width="80" />
            <Button x:Name="ButtonTovar2RaschetStoimosti5" RelativePanel.Below="TextBoxTovar1Naimenovanie5" RelativePanel.RightOf="TextBoxTovar2CenaEd5" Margin="20,15,0,0" Content=">" Click="ButtonTovar2RaschetStoimosti5_Click" />
            <TextBox x:Name="TextBoxTovar2Stoimost5" RelativePanel.Below="TextBoxTovar1Naimenovanie5" RelativePanel.RightOf="ButtonTovar2RaschetStoimosti5" Margin="20,15,0,20" Width="80" />
            <CheckBox x:Name="CheckBoxTovar2" RelativePanel.Below="TextBoxTovar1Naimenovanie5" RelativePanel.RightOf="TextBoxTovar2Stoimost5" Margin="40,15,0,0" />
            <Button x:Name="AddTovar2Next" RelativePanel.Below="TextBoxTovar2Naimenovanie5" Margin="20,5,0,0" Background="#FF99BC64" Foreground="White" Content="Добавить товар/услугу" Click="AddTovar2Next_Click"/>
            <Button x:Name="RemoveTovar2Next" RelativePanel.Below="TextBoxTovar2Naimenovanie5" RelativePanel.RightOf="AddTovar2Next" Margin="20,5,0,0" Background="#FF99BC64" Foreground="White" Content="Убрать товар/услугу" Click="RemoveTovar2Next_Click"/>


            <TextBox x:Name="TextBoxTovar3Naimenovanie5" RelativePanel.Below="TextBoxTovar2Naimenovanie5" Width="230" Margin="20,15,0,0" />
            <TextBox x:Name="TextBoxTovar3Kolvo5" RelativePanel.Below="TextBoxTovar2Naimenovanie5" RelativePanel.RightOf="TextBoxTovar1Naimenovanie5" Margin="20,15,0,0" />
            <TextBox x:Name="TextBoxTovar3Ed5" RelativePanel.Below="TextBoxTovar2Naimenovanie5" RelativePanel.RightOf="TextBoxTovar1Kolvo5" Margin="20,15,0,0" />
            <TextBox x:Name="TextBoxTovar3CenaEd5" RelativePanel.Below="TextBoxTovar2Naimenovanie5" RelativePanel.RightOf="TextBoxTovar1Ed5" Margin="20,15,0,0" Width="80" />
            <Button x:Name="ButtonTovar3RaschetStoimosti5" RelativePanel.Below="TextBoxTovar2Naimenovanie5" RelativePanel.RightOf="TextBoxTovar3CenaEd5" Margin="20,15,0,0" Content=">" Click="ButtonTovar3RaschetStoimosti5_Click" />
            <TextBox x:Name="TextBoxTovar3Stoimost5" RelativePanel.Below="TextBoxTovar2Naimenovanie5" RelativePanel.RightOf="ButtonTovar3RaschetStoimosti5" Margin="20,15,0,20" Width="80" />
            <CheckBox x:Name="CheckBoxTovar3" RelativePanel.Below="TextBoxTovar2Naimenovanie5" RelativePanel.RightOf="TextBoxTovar3Stoimost5" Margin="40,15,0,0" />
            <Button x:Name="AddTovar3Next" RelativePanel.Below="TextBoxTovar3Naimenovanie5" Margin="20,5,0,0" Background="#FF99BC64" Foreground="White" Content="Добавить товар/услугу" Click="AddTovar3Next_Click"/>
            <Button x:Name="RemoveTovar3Next" RelativePanel.Below="TextBoxTovar3Naimenovanie5" RelativePanel.RightOf="AddTovar3Next" Margin="20,5,0,0" Background="#FF99BC64" Foreground="White" Content="Убрать товар/услугу" Click="RemoveTovar3Next_Click"/>


            <TextBox x:Name="TextBoxTovar4Naimenovanie5" RelativePanel.Below="TextBoxTovar3Naimenovanie5" Width="230" Margin="20,15,0,0" />
            <TextBox x:Name="TextBoxTovar4Kolvo5" RelativePanel.Below="TextBoxTovar3Naimenovanie5" RelativePanel.RightOf="TextBoxTovar1Naimenovanie5" Margin="20,15,0,0" />
            <TextBox x:Name="TextBoxTovar4Ed5" RelativePanel.Below="TextBoxTovar3Naimenovanie5" RelativePanel.RightOf="TextBoxTovar1Kolvo5" Margin="20,15,0,0" />
            <TextBox x:Name="TextBoxTovar4CenaEd5" RelativePanel.Below="TextBoxTovar3Naimenovanie5" RelativePanel.RightOf="TextBoxTovar1Ed5" Margin="20,15,0,0" Width="80" />
            <Button x:Name="ButtonTovar4RaschetStoimosti5" RelativePanel.Below="TextBoxTovar3Naimenovanie5" RelativePanel.RightOf="TextBoxTovar4CenaEd5" Margin="20,15,0,0" Content=">" Click="ButtonTovar4RaschetStoimosti5_Click" />
            <TextBox x:Name="TextBoxTovar4Stoimost5" RelativePanel.Below="TextBoxTovar3Naimenovanie5" RelativePanel.RightOf="ButtonTovar4RaschetStoimosti5" Margin="20,15,0,20" Width="80" />
            <CheckBox x:Name="CheckBoxTovar4" RelativePanel.Below="TextBoxTovar3Naimenovanie5" RelativePanel.RightOf="TextBoxTovar4Stoimost5" Margin="40,15,0,0" />
            <Button x:Name="AddTovar4Next" RelativePanel.Below="TextBoxTovar4Naimenovanie5" Margin="20,5,0,0" Background="#FF99BC64" Foreground="White" Content="Добавить товар/услугу" Click="AddTovar4Next_Click"/>
            <Button x:Name="RemoveTovar4Next" RelativePanel.Below="TextBoxTovar4Naimenovanie5" RelativePanel.RightOf="AddTovar4Next" Margin="20,5,0,0" Background="#FF99BC64" Foreground="White" Content="Убрать товар/услугу" Click="RemoveTovar4Next_Click"/>


            <TextBox x:Name="TextBoxTovar5Naimenovanie5" RelativePanel.Below="TextBoxTovar4Naimenovanie5" Width="230" Margin="20,15,0,0" />
            <TextBox x:Name="TextBoxTovar5Kolvo5" RelativePanel.Below="TextBoxTovar4Naimenovanie5" RelativePanel.RightOf="TextBoxTovar1Naimenovanie5" Margin="20,15,0,0" />
            <TextBox x:Name="TextBoxTovar5Ed5" RelativePanel.Below="TextBoxTovar4Naimenovanie5" RelativePanel.RightOf="TextBoxTovar1Kolvo5" Margin="20,15,0,0" />
            <TextBox x:Name="TextBoxTovar5CenaEd5" RelativePanel.Below="TextBoxTovar4Naimenovanie5" RelativePanel.RightOf="TextBoxTovar1Ed5" Margin="20,15,0,0" Width="80" />
            <Button x:Name="ButtonTovar5RaschetStoimosti5" RelativePanel.Below="TextBoxTovar4Naimenovanie5" RelativePanel.RightOf="TextBoxTovar5CenaEd5" Margin="20,15,0,0" Content=">" Click="ButtonTovar5RaschetStoimosti5_Click" />
            <TextBox x:Name="TextBoxTovar5Stoimost5" RelativePanel.Below="TextBoxTovar4Naimenovanie5" RelativePanel.RightOf="ButtonTovar5RaschetStoimosti5" Margin="20,15,0,20" Width="80" />
